{
  "_version": "1.76.0",
  "sap.app": {
    "id": "com.deloitte.mdg.salepricing.initiator.initiator",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.20.0",
      "toolsId": "83b2e41e-c561-48e5-b0c1-cd003e56da27"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/API_SLSPRICINGCONDITIONRECORD_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "2.0",
          "localUri": "localService/metadata.xml"
        }
      },
      "SAPService": {
        "uri": "/sap/opu/odata/sap/ZMDG_SLSPRICINGCONDITIONRECORD_SRV/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "DG_SalePricing-Display": {
          "semanticObject": "DG_SalePricing",
          "action": "Display",
          "title": "{{DG_SalePricing-Display.flpTitle}}",
          "subTitle": "{{DG_SalePricing-Display.flpSubtitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.143.2",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.deloitte.mdg.salepricing.initiator.initiator.i18n.i18n"
        }
      },
      "MainServiceModel": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "SAPSalesModel": {
        "dataSource": "SAPService",
        "settings": {
          "operationMode": "Server",
          "earlyRequests": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "com.deloitte.mdg.salepricing.initiator.initiator.view",
        "async": true,
        "viewPath": "com.deloitte.mdg.salepricing.initiator.initiator.view"
      },
      "routes": [
        {
          "name": "RouteOverview",
          "pattern": ":?query:",
          "target": [
            "TargetOverview"
          ]
        },
        {
          "name": "RouteSubmission",
          "pattern": "submission/{request_type}/:ca:/:cc:/:ve:/:request_id:",
          "greedy": true,
          "target": [
            "TargetSubmission"
          ]
        },
        {
          "name": "RouteCreate",
          "pattern": "Create/{data}",
          "target": [
            "TargetCreate"
          ]
        },
        {
          "name": "RouteDisplay",
          "pattern": "Display",
          "target": [
            "TargetDisplay"
          ]
        }
      ],
      "targets": {
        "TargetOverview": {
          "id": "Overview",
          "name": "Overview"
        },
        "TargetSubmission": {
          "viewType": "XML",
          "transition": "slide",
          "viewId": "Submission",
          "viewName": "Submission"
        },
        "TargetCreate": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "id": "Create",
          "name": "Create"
        },
        "TargetDisplay": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "id": "Display",
          "name": "Display"
        }
      }
    },
    "rootView": {
      "viewName": "com.deloitte.mdg.salepricing.initiator.initiator.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    },
    "sap.cloud": {
      "public": true,
      "service": "DG-SalesPricing"
    }
  }
}
