<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">

    <Dialog
        id="resultDialog"
        title="{result>/title}"
        type="Message"
        state="{result>/state}">

        <content>
            <Text id="_IDGenText1" text="{result>/message}" />
        </content>

        <beginButton>
            <Button id="_IDGenButton"
                text="OK"
                type="Emphasized"
                press=".onResultDialogClose" />
        </beginButton>

    </Dialog>
</core:FragmentDefinition>
